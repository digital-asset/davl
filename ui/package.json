  "private": true,
  "dependencies": {
    "@digitalasset/daml-json-types": "0.0.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "fp-ts": "^2.1.1",
    "jwt-simple": "^0.5.6",
    "react": "^16.10.2",
    "react-async": "^9.0.0",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-scripts": "3.2.0",
    "react-semantic-toasts": "https://github.com/hurryabit/react-semantic-toasts.git#c606bdee68a21b939c5509e6882810431a50222f",
    "redux": "^4.0.4",
    "redux-starter-kit": "^1.0.0",
    "redux-thunk-error-handler": "^1.1.3",
    "semantic-ui-calendar-react": "^0.15.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --testURL='http://localhost:7575'",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-floating-promises": "error",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-inferrable-types": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:7575",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/react": "^5.2.4",
    "@types/jwt-simple": "^0.5.33",
    "@types/react-redux": "^7.1.5",
    "@types/redux-mock-store": "^1.0.1",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/parser": "^2.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.6.0",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "redux-mock-store": "^1.5.3"
  }
}
